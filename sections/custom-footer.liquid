<div class="custom-footer">
  <div class="custom-footer--navContainer">
    {% for block in section.blocks %}
      {{ block.settings.title }}
      {%- if block.settings.list != blank -%}
        <ul class="custom-footer-navContainer--block" >
          {%- for link in block.settings.list.links -%}
            <li style="list-style: none;">
              <a
                href="{{ link.url }}"
              >
                {{ link.title }}
              </a>
            </li>
          {%- endfor -%}
        </ul>
      {% endif %}  
    {% endfor %}  
  </div>
</div>  


{% schema %}
{
  "name": "custom Footer section",
  "tag": "section",
  "class": "section",
  "settings": [
    {
        "type": "image_picker",
        "id": "desktop_image",
        "label": "Store image"
    }
  ],
  "blocks": [
    {
      "type": "footer-block",
      "name": "Footer block",
      "settings": [
        {
            "type": "text",
            "id": "title",
            "label": "Title",
            "placeholder": "Contact"
        },
        {
            "type": "link_list",
            "id": "list",
            "label": "options"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "custom Footer section"
    }
  ]
}
{% endschema %}